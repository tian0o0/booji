{
  "name": "@booji/react",
  "version": "0.3.9",
  "description": "@booji/react",
  "author": "xietian",
  "license": "BSD-3-Clause",
  "main": "./dist/react.cjs.js",
  "module": "./dist/react.esm.js",
  "types": "./dist/react.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && node ../../build/api-extractor.js react",
    "lint": "prettier --write src/**/*"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "diagnostics": false
      }
    }
  },
  "dependencies": {
    "@booji/browser": "0.3.9",
    "@booji/types": "0.3.9",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}

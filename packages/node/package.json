{
  "name": "@booji/node",
  "version": "1.2.2",
  "description": "@booji/node",
  "author": "xietian",
  "license": "BSD-3-Clause",
  "main": "./dist/node.cjs.js",
  "module": "./dist/node.esm.js",
  "types": "./dist/node.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && node ../../build/api-extractor.js node",
    "lint": "prettier --write src/**/*"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "diagnostics": false
      }
    }
  },
  "dependencies": {
    "@booji/core": "1.2.2",
    "@booji/hub": "1.2.2",
    "@booji/types": "1.2.2",
    "@booji/utils": "1.2.2",
    "tslib": "^2.3.1"
  }
}
